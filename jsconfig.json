{
    "compilerOptions": {
    "target": "ES2022",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "jsx": "react-jsx",
    "allowImportingTsExtensions": true,
    "strictNullChecks": true,
    "strictFunctionTypes": true,
    },
    "include": ["src/**/*"],
    "exclude": [
        "node_modules",
        "**/node_modules/*"
    ]
}